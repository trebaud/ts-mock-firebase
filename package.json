  "license": "Apache-2.0",
  "devDependencies": {
    "@types/antlr4": "4.7.0",
    "@types/jest": "25.2.1",
    "@types/uuid": "7.0.3",
    "jest": "25.5.4",
    "rimraf": "3.0.2",
    "ts-jest": "25.4.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "1.18.0",
    "tslint-sonarts": "1.9.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@firebase/app-types": "0.6.0",
    "@firebase/auth": "0.14.4",
    "@firebase/firestore": "1.17.1",
